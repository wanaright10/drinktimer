{
  "expo": {
    "name": "喝水提醒",
    "description": "一个提醒您定时喝水的软件!",
    "slug": "drinktimer",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": [
      "ios"
    ],
    "version": "1.0.1",
